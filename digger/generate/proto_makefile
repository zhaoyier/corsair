gen_proto: service/internal/rpc/*.pb.go
clean_proto:
	rm -f service/internal/rpc/*.pb.go
service/internal/rpc/*.pb.go: ../apidoc/proto/digger/*.proto $(shell which protoc) $(shell which protoc-gen-go)
	@mkdir -p service/internal/rpc
	@echo "gen proto"
	@rm -f service/internal/rpc/*.pb.go
	@protoc -I ../apidoc/proto ../apidoc/proto/digger/*.proto --go_out=plugins=grpc:service/internal/rpc
